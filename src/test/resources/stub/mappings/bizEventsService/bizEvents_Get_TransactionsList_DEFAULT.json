{
  "request": {
    "method": "GET",
    "urlPathPattern": "/bizEventsMock/transactions",
    "headers": {
      "x-api-key": {
        "matches": "x_api_key([0-9]?)+"
      },
      "x-fiscal-code": {
        "matches": "DUMMY_FISCAL_CODE_DEFAULT"
      },
      "x-continuation-token": {
        "matches": "TOKEN"
      }
    },
    "queryParameters": {
      "size": {
        "equalTo": "2"
      }
    }
  },
  "response": {
    "status": 500
  }
}
