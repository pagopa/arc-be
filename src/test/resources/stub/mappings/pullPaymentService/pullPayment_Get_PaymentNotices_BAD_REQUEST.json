{
  "request": {
    "method": "GET",
    "urlPathPattern": "/pullPaymentMock/payment-notices/v1",
    "headers": {
      "Ocp-Apim-Subscription-Key": {
        "matches": "x_api_key([0-9]?)+"
      },
      "x-tax-code": {
        "matches": "DUMMY_FISCAL_CODE_BAD_REQUEST"
      }
    },
    "queryParameters": {
      "limit": {
        "equalTo": "1000"
      },
      "page": {
        "equalTo": "0"
      }
    }
  },
  "response": {
    "status": 400
  }
}